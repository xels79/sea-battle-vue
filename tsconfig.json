{
    "compilerOptions": {
        "target": "ES2022",
        "lib":["es2022", "dom"],
        "module": "ESNext",
        "sourceMap":true,
        "moduleResolution": "Node",
        "outDir": ".",
        "experimentalDecorators": false,
        "strictPropertyInitialization": false,
        "declaration": false,
        "types": ["node"],
        "esModuleInterop": true,
        "isolatedModules":true,
        "verbatimModuleSyntax":true,
        "strict":true
    }, 
    "files":[
        "client/main.ts"
    ],
    "exclude": ["node_modules"],
    "extends": ["./tsconfig_ex.json", "@vue/tsconfig/tsconfig.dom.json"]
}